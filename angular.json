{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angdemo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angdemo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/jquery-1.11.3.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.repeater.js",
              "src/assets/js/perfect-scrollbar.js",
              "src/assets/js/bootstrap-datetimepicker.js",
              "src/assets/js/jquery.dataTables.min.js",
              "src/assets/js/sweetalert2.min.js",
              "src/assets/js/custom.js",
              "src/assets/js/angular.js",
              "src/assets/js/dataTables.buttons.min.js",
              "src/assets/js/dataTables.responsive.min.js",
              "src/assets/js/buttons.print.min.js",
              "src/assets/js/jszip.min.js",
              "src/assets/js/buttons.html5.min.js",
              "src/assets/js/jspdf.min.js",
              "src/assets/js/html2canvas.min.js",
              "src/assets/js/select2.min.js",
              "src/assets/js/typeahead.bundle.js",
              "src/assets/js/jquery.bxslider.js",
              "src/assets/js/jquery.flexslider.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ts"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angdemo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angdemo:build:production"
            },
            "dev": {
              "browserTarget": "angdemo:build:dev"
            },
            "local": {
              "browserTarget": "angdemo:build:local"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angdemo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angdemo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angdemo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angdemo:serve:production"
            },
            "dev": {
              "devServerTarget": "angdemo:serve:dev"
            },
            "local": {
              "devServerTarget": "angdemo:serve:local"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "angdemo"
}